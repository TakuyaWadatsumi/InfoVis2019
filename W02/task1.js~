Vec3 = function( x, y, z )
{
    this.x = x;
    this.y = y;
    this.z = z;
}

Vec3.prototype.min = function()
{  
    var a = Math.min(this.x,this.y,this.z);
    return a;
}


Vec3.prototype.sum = function()
{
    a = this.x;
    b = this.y;
    c = this.z;
    var data = [a,b,c];
    return a;
}
